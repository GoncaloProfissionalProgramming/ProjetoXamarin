<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Projeto.Register">

    <StackLayout VerticalOptions="Center">


        <Entry x:Name="Et_Name" Placeholder="Name" />
        <Entry x:Name="Et_Username" Placeholder="Username" />
        <Entry x:Name="Et_Password" Placeholder="Password" IsPassword="True"/>
        
        <Button x:Name="Bt_Register" Text="Register" Clicked="Bt_Register_Clicked" />

    </StackLayout>

</ContentPage>